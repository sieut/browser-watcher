{
  "manifest_version": 2,

  "name": "Browser Watcher",
  "description": "This extension tracks how much time you spend on each website",
  "version": "0.1",

  "browser_action": {
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "<all_urls>"
  ]
}